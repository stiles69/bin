function PullMachine ()
{
	local MACHINENAME="$1"
	ssh brettsalemink@"$MACHINENAME" "$HOME/bin/Pull-Bin-Sh.sh"
	wait
	echo "Finished Syncing $MACHINENAME"
}	# end PullMachine

function Main ()
{
